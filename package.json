{
  "name": "allinone",
	"displayName": "allinone",
	"description": "",
	"version": "0.0.3",
	"repository": {
        "url": "https://github.com/hangj/vscode_extension"
    },
	"engines": {
		"vscode": "^1.80.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:allinone.helloWorld",
		"onCommand:allinone.insert_line_number", 
		"onCommand:allinone.auto_multi_select",
		"onCommand:allinone.auto_indent",
		"onCommand:allinone.sum"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "allinone.helloWorld",
				"title": "allinone: Hello World"
			},
			{
				"command": "allinone.insert_line_number",
				"title": "allinone: insert line number"
			},
			{
				"command": "allinone.auto_multi_select",
				"title": "allinone: auto multi select"
			},
			{
				"command": "allinone.auto_indent",
				"title": "allinone: auto indent"
			}
		],
		"keybindings": [
			{
				"command": "allinone.auto_multi_select",
				"key": "ctrl+shift+a",
				"mac": "cmd+shift+a",
				"when": "editorHasSelection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.80.0",
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "20.2.5",
		"glob": "^8.1.0",
		"mocha": "^10.2.0",
		"typescript": "^5.1.3",
		"@vscode/test-electron": "^2.3.2"
	}
}
